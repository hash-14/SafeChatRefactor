<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/chat_msg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="6dp"
        android:background="@drawable/buttonchattgray"
        android:maxWidth="300dp"
        android:paddingBottom="8dp"
        android:paddingEnd="6dp"
        android:paddingLeft="6dp"
        android:paddingRight="6dp"
        android:paddingStart="6dp"
        android:paddingTop="8dp"
        android:text="Loading..."
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="16sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="8dp" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="6dp"
        android:background="@drawable/buttonchatt"
        android:maxWidth="300dp"
        android:paddingBottom="8dp"
        android:paddingEnd="6dp"
        android:paddingLeft="6dp"
        android:paddingRight="6dp"
        android:paddingStart="6dp"
        android:paddingTop="8dp"
        android:text="Loading..."
        android:textColor="@color/blackito"
        android:textSize="16sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/chat_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:text="Loading..."
        android:textSize="11sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/chat_msg"
        app:layout_constraintTop_toBottomOf="@+id/chat_msg"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginStart="8dp" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="6dp"
        android:text="Loading..."
        android:textSize="11sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="@+id/textView31"
        app:layout_constraintTop_toBottomOf="@+id/textView31"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginEnd="6dp" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:background="@drawable/ic_fiber_gray"
        app:layout_constraintLeft_toRightOf="@+id/chat_time"
        android:layout_marginLeft="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/chat_time"
        app:layout_constraintTop_toTopOf="@+id/chat_time"
        android:layout_marginStart="8dp" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:background="@drawable/ic_fiber_gray"
        app:layout_constraintTop_toTopOf="@+id/textView33"
        app:layout_constraintBottom_toBottomOf="@+id/textView33"
        app:layout_constraintRight_toLeftOf="@+id/textView33"
        android:layout_marginRight="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/roundbuttonchatbluedelayed"
        android:text="Loading..."
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginRight="4dp"
        android:layout_marginTop="4dp"
        android:adjustViewBounds="true"
        android:cropToPadding="true"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="@+id/chat_msg"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/chat_msg"
        app:layout_constraintTop_toTopOf="@+id/chat_msg"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@color/c4"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        app:layout_constraintRight_toRightOf="@+id/chat_msg" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginRight="4dp"
        android:layout_marginTop="4dp"
        android:adjustViewBounds="true"
        android:cropToPadding="true"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="@+id/textView31"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/textView31"
        app:layout_constraintRight_toRightOf="@+id/textView31"
        app:layout_constraintTop_toTopOf="@+id/textView31"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@color/c4"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp" />

    <ProgressBar
        android:id="@+id/progressBar2"
        style="?android:attr/progressBarStyle"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="4dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="4dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/textView31"
        app:layout_constraintHorizontal_bias="0.526"
        app:layout_constraintLeft_toLeftOf="@+id/imageView17"
        app:layout_constraintRight_toRightOf="@+id/textView31"
        app:layout_constraintTop_toTopOf="@+id/textView31"
        app:layout_constraintVertical_bias="0.461" />

    <android.support.constraint.ConstraintLayout
        android:id="@+id/audiohim"
        android:layout_width="230dp"
        android:layout_height="0dp"
        android:layout_marginTop="2dp"
        android:background="@drawable/roundbuttongray"
        app:layout_constraintBottom_toBottomOf="@+id/chat_msg"
        app:layout_constraintLeft_toLeftOf="@+id/chat_msg"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <ProgressBar
            android:id="@+id/progressBar3"
            style="?android:attr/progressBarStyle"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginBottom="8dp"
            android:layout_marginTop="8dp"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="@+id/button581c"
            app:layout_constraintLeft_toLeftOf="@+id/button581c"
            app:layout_constraintRight_toRightOf="@+id/button581c"
            app:layout_constraintTop_toTopOf="@+id/button581c" />

        <TextView
            android:id="@+id/textView60"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:text="Loading..."
            android:textSize="9sp"
            app:layout_constraintLeft_toLeftOf="@+id/seekBar1c"
            app:layout_constraintTop_toBottomOf="@+id/seekBar1c"
            android:layout_marginStart="8dp" />

        <SeekBar
            android:id="@+id/seekBar1c"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="8dp"
            app:layout_constraintLeft_toRightOf="@+id/button581c"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="4dp"
            android:layout_marginEnd="8dp" />

        <Button
            android:id="@+id/button581c"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"
            android:background="@drawable/ic_mic_greenn"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/audiome"
        android:layout_width="230dp"
        android:layout_height="0dp"
        android:layout_marginTop="2dp"
        android:layout_marginRight="8dp"
        android:background="@drawable/roundbutton2"
        app:layout_constraintBottom_toBottomOf="@+id/textView31"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <ProgressBar
            android:id="@+id/progressBar4"
            style="?android:attr/progressBarStyle"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="@+id/button581"
            app:layout_constraintRight_toRightOf="@+id/button581"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView61"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:text="Loading..."
            android:textColor="@color/blackito"
            android:textSize="9sp"
            app:layout_constraintLeft_toRightOf="@+id/button581"
            app:layout_constraintTop_toBottomOf="@+id/seekBar1" />

        <SeekBar
            android:id="@+id/seekBar1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginBottom="4dp"
            android:background="@android:color/transparent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toRightOf="@+id/button581"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button581"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:background="@drawable/ic_mic_orange"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.555" />
    </android.support.constraint.ConstraintLayout>

    <TextView
        android:id="@+id/textView62"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:text="Loading..."
        android:textSize="11sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toRightOf="@+id/imageView13"
        app:layout_constraintTop_toTopOf="@+id/chat_time"
        android:layout_marginStart="8dp" />

    <TextView
        android:id="@+id/textView63"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Loading..."
        android:textSize="11sp"
        android:textStyle="bold"
        app:layout_constraintRight_toLeftOf="@+id/imageView18"
        android:layout_marginRight="8dp"
        app:layout_constraintTop_toTopOf="@+id/textView33"
        android:layout_marginEnd="8dp" />

    <LinearLayout
        android:id="@+id/hjs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        android:background="@drawable/buttonchattgray2"
        android:orientation="horizontal"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/audiome"
        app:layout_constraintRight_toRightOf="parent">

        <Button
            android:id="@+id/button75s"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginEnd="5dp"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:background="@drawable/ic_menu_white_24dp"
            app:layout_constraintBottom_toBottomOf="@+id/audiome"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/audiome"
            app:layout_constraintVertical_bias="0.428" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/hj"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/buttonchattgray2"
        android:orientation="horizontal"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/audiohim"
        app:layout_constraintLeft_toLeftOf="@+id/audiohim">

        <Button
            android:id="@+id/button75"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginEnd="5dp"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:background="@drawable/ic_menu_white_24dp"
            app:layout_constraintBottom_toBottomOf="@+id/audiome"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/audiome"
            app:layout_constraintVertical_bias="0.428" />
    </LinearLayout>

</android.support.constraint.ConstraintLayout>